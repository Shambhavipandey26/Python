import array

arr = array.array('i', [1, 2, 2, 2, 5, 5, 6, 6, 3, 3])
print("array")
print()
for i in range(len(arr)):
    print(arr[i],end="")
print()
a=eval(input("Enter the number:"))
for i in range(len(arr)):
    if a==arr[i]:
        arr.pop(i)
        break;
for i in range(len(arr)):
    print(arr[i],end="")
print()
